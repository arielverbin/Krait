
def initinit(self, arg):
    self.next = None

def print_list(self):
    result = "[" + str(self)
    self = self.next

    if None == self:
        result = result + "]"
        print result
        return
    else:
        result = result + ", "

    while None != self.next:
        result = result + str(self) + ", "
        self = self.next
    
    result = result + str(self) + "]"
    print result

def get_element(self, index):
    while None != self and index > 0:
        self = self.next
        index = index - 1
    
    return self

def length(self):
    l = 0
    while None != self:
        l = l + 1
        self = self.next

    return l

int.__init__ = initinit
int.print_list = print_list
int.get_element = get_element
int.length = length

